{
  "name": "@mintlify/config",
  "private": "true",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "publish:eslint-config": "npm publish --workspace @mintlify/eslint-config",
    "publish:eslint-config-typescript": "npm publish --workspace @mintlify/eslint-config-typescript",
    "publish:eslint-config-next": "npm publish --workspace @mintlify/eslint-config-next",
    "publish:prettier-config": "npm publish --workspace @mintlify/prettier-config",
    "publish:ts-config": "npm publish --workspace @mintlify/ts-config"
  }
}
